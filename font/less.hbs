@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

.{{baseClass}} {
	line-height: 1;
}

.{{baseClass}}:before {
	font-family: {{fontName}};
	font-style: normal;
	font-weight: normal;
	vertical-align: top;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}